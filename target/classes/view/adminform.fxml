<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="adminanchor" prefHeight="401.0" prefWidth="709.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.prison.controller.AdminformController">
    <children>
        <ImageView disable="true" fitHeight="556.0" fitWidth="713.0" pickOnBounds="true" preserveRatio="true" smooth="false">
            <image>
                <Image url="@image/bigstock-Rows-of-prison-cells-prison-i-115203026.jpg" />
            </image>
            <cursor>
                <Cursor fx:constant="NONE" />
            </cursor>
        </ImageView>
      <TextField fx:id="oldPassword" blendMode="DIFFERENCE" focusTraversable="false" layoutX="256.0" layoutY="201.0" onAction="#oldPassOnAction" opacity="0.7" prefHeight="25.0" prefWidth="202.0" promptText="Old Password">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></TextField>
        <TextField fx:id="username" blendMode="DIFFERENCE" focusTraversable="false" layoutX="256.0" layoutY="243.0" onAction="#usernameONAction" opacity="0.7" prefHeight="25.0" prefWidth="202.0" promptText=" New User name" />
      <PasswordField fx:id="password" blendMode="DIFFERENCE" focusTraversable="false" layoutX="256.0" layoutY="287.0" onAction="#passOnAction" opacity="0.7" prefHeight="25.0" prefWidth="202.0" promptText=" New Password" />
        <Button fx:id="btnsave" blendMode="DIFFERENCE" focusTraversable="false" layoutX="334.0" layoutY="331.0" mnemonicParsing="false" onAction="#btnlsaveOnaction" opacity="0.8" text="Save">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <ImageView fitHeight="68.0" fitWidth="67.0" layoutX="323.0" layoutY="114.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@image/user.png" />
            </image>
        </ImageView>
        <Button fx:id="btnback" blendMode="DIFFERENCE" focusTraversable="false" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnlbackOnaction" opacity="0.7" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="logout" blendMode="DIFFERENCE" focusTraversable="false" layoutX="641.0" layoutY="365.0" mnemonicParsing="false" onAction="#btnlogoutOnaction" opacity="0.7" text="Logout">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
</AnchorPane>
